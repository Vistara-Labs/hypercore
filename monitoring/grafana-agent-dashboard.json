{
  "dashboard": {
    "title": "Claude Agent SDK - Production Dashboard",
    "tags": ["claude", "agents", "hypercore"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Active Agent Sessions",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "agent_active_sessions",
            "legendFormat": "Active Sessions",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Sessions"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_requests_total[1m])",
            "legendFormat": "{{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Request Latency (p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(agent_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Token Usage (input/output)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_token_usage_total{type=\"input\"}[5m])",
            "legendFormat": "Input Tokens/sec",
            "refId": "A"
          },
          {
            "expr": "rate(agent_token_usage_total{type=\"output\"}[5m])",
            "legendFormat": "Output Tokens/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Hypercore Agent Spawn Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(hypercore_agent_spawn_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Spawns/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Active Hypercore Agents",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "hypercore_agent_active_count",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 8,
        "title": "Total Requests (24h)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(agent_requests_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 9,
        "title": "Total Tokens (24h)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(agent_token_usage_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        }
      },
      {
        "id": 10,
        "title": "Success Rate (%)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "100 * rate(agent_requests_total{status=\"success\"}[5m]) / rate(agent_requests_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "unit": "percent"
        }
      },
      {
        "id": 11,
        "title": "Agent Spawn Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(hypercore_agent_spawn_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 Spawn Time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Memory Usage per Agent",
        "type": "graph",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "container_memory_usage_bytes{container=\"claude-agent\"}",
            "legendFormat": "{{agent_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      }
    ]
  }
}
